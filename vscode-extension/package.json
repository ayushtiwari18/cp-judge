{
  "name": "cp-judge-vscode",
  "displayName": "CP Judge",
  "description": "Run competitive programming code using local CP Judge server",
  "version": "0.1.0",
  "publisher": "ayush5410",
  "author": {
    "name": "Ayush Tiwari",
    "url": "https://github.com/ayushtiwari18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ayushtiwari18/cp-judge"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages",
    "Testing"
  ],
  "keywords": [
    "competitive programming",
    "judge",
    "testing",
    "codeforces",
    "leetcode"
  ],
  "activationEvents": [
    "onCommand:cpjudge.run"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cpjudge.run",
        "title": "CP Judge: Run"
      }
    ],
    "keybindings": [
      {
        "command": "cpjudge.run",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r",
        "when": "editorTextFocus"
      }
    ]
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0"
  },
  "icon": "icon.png",
  "license": "MIT"
}
